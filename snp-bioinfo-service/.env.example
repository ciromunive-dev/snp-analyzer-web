# Redis (Upstash)
# Para desarrollo local, usa redis://localhost:6379
# Para producción, usa la URL de Upstash
UPSTASH_REDIS_REST_URL=https://xxx.upstash.io
UPSTASH_REDIS_REST_TOKEN=xxx

# Supabase
# Obtener desde: https://supabase.com/dashboard/project/_/settings/api
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_SERVICE_KEY=eyJ...  # Usar service_role key, NO anon key

# NCBI
# Requerido para usar BLAST y otras APIs de NCBI
# Registrarse en: https://www.ncbi.nlm.nih.gov/account/
NCBI_EMAIL=tu-email@ejemplo.com
# Opcional pero recomendado para más requests (10/s vs 3/s)
NCBI_API_KEY=

# Worker
WORKER_SLEEP_INTERVAL=5
BLAST_TIMEOUT=120

# App
DEBUG=false
